@model _08_CRUDpersonasTelefonos_UI.Models.ViewModels.CambiarTelefonoViewModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CambiarTelefono</title>
</head>
<body>
    @using (Html.BeginForm())
    {
        <div>
            <h4>clsPersonaConNombreDeDepartamento</h4>
            <hr />
            <dl class="dl-horizontal">



                @Html.HiddenFor(model => model.personaConNombreDeDepartamento.idPersona)
                @Html.HiddenFor(model => model.personaConNombreDeDepartamento.fechaNacimiento)
                @Html.HiddenFor(model => model.personaConNombreDeDepartamento.direccion)
                @Html.HiddenFor(model => model.personaConNombreDeDepartamento.idDepartamento)

                <dt>
                    @Html.DisplayNameFor(model => model.personaConNombreDeDepartamento.nombre)
                    @Html.HiddenFor(model => model.personaConNombreDeDepartamento.nombre)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.personaConNombreDeDepartamento.nombre)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.personaConNombreDeDepartamento.apellidos)
                    @Html.HiddenFor(model => model.personaConNombreDeDepartamento.apellidos)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.personaConNombreDeDepartamento.apellidos)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.personaConNombreDeDepartamento.nombreDepartamento)
                    @Html.HiddenFor(model => model.personaConNombreDeDepartamento.nombreDepartamento)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.personaConNombreDeDepartamento.nombreDepartamento)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.personaConNombreDeDepartamento.telefono)
                </dt>

                <dd>
                    @Html.EditorFor(model => model.personaConNombreDeDepartamento.telefono)
                </dd>



            </dl>
        </div>
        <p>
            <input type="submit" value="Guardar"/>
        </p>

        <p>
            @*@Html.ActionLink("Guardar", "CambiarTelefono", new { id = Model.personaConNombreDeDepartamento.idPersona }) |*@
            @*@Html.ActionLink("Guardar", "CambiarTelefono", new { personaConNombreDeDepartamento = Model.personaConNombreDeDepartamento }) |*@
            @*@Html.ActionLink("Guardar", "CambiarTelefono", new { id = Model.personaConNombreDeDepartamento.idPersona, telefono = Model.personaConNombreDeDepartamento.telefono }) |*@
            @Html.ActionLink("Volver", "regresoTablaConIdDepartamento") @*, new { id = Model.personaConNombreDeDepartamento.idDepartamento }*@
        </p>
    }
</body>
</html>
